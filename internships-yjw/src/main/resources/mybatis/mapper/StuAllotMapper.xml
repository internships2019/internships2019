<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ylq.internships.mapper.StuAllotMapper">
    <resultMap id="stuAllot" type="StuAllot">
        <id property="id" column="id" javaType="Integer"/>
        <result property="sWxNo" column="s_wx_no" javaType="String"/>
        <result property="state" column="state" javaType="String"/>
        <result property="comName" column="com_name" javaType="String"/>
        <result property="comLocation" column="com_location" javaType="String"/>
        <result property="comTel" column="com_tel" javaType="String"/>
        <result property="jobId" column="job_id" javaType="Integer"/>
        <result property="jobName" column="job_name" javaType="String"/>
        <result property="scTeaWx" column="sc_tea_wx" javaType="String"/>
        <result property="comTeaWx" column="com_tea_wx" javaType="String"/>
        <result property="scTeaName" column="sc_tea_name" javaType="String"/>
        <result property="comTeaName" column="com_tea_name" javaType="String"/>
    </resultMap>

    <insert id="insertStu" parameterType="StuAllot">
        INSERT INTO `stu_allot` ( `s_wx_no`, `state`, `com_name`, `com_location`, `com_tel`, `job_id`, `job_name`, `s_sc_name` )
        VALUES ( #{sWxNo},#{state},#{comName},#{comLocation},#{comTel},#{jobId},#{jobName},#{sScName})
    </insert>

    <select id="selectByStuId" resultType="StuAllot">
        select * from `stu_allot` where `s_wx_no` = #{openid}
    </select>

    <select id="getApplications" resultMap="stuAllot">
        select * from `stu_allot` where `s_wx_no` = #{openid};
    </select>
</mapper>