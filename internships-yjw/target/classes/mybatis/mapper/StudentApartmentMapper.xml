<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ylq.internships.mapper.StudentApartmentMapper">

    <!--应改为studentapartment-->
    <resultMap id="studentApartment" type="StudentApartment">
        <id property="id" column="id" javaType="Integer"/>
        <result property="liveLocation" column="live_location" javaType="String"/>
        <result property="sWx" column="s_wx" javaType="String"/>
        <result property="liveLongtitude" column="live_longtitude" javaType="Double"/>
        <result property="liveLatitude" column="live_latitude" javaType="Double"/>
    </resultMap>

    <select id="getLocation" resultMap="studentApartment">
        select * from `student_apartment` where `s_wx` = #{openid};
    </select>
</mapper>